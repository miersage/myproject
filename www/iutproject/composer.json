{
    "name": "iutproject/iutproject",
    "type": "project",
    "description": "iutproject",
    "keywords": [
        "slim-framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "monolog/monolog": "^2.4",
        "php-di/php-di": "^6.3",
        "slim/psr7": "^1.2",
        "slim/slim": "^4.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4 || ^9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
